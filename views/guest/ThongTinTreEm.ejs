    <link href="/stylesheets/common.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css" media="screen">

    <%-include("./menu_tttreem")%>
    <div class="fmcenter">
        <div class="TNContent">
            <div id="dnn_Top" class="DNNEmptyPane">
            </div>
            <div class="clear">
            </div>
            <div class="TNMain">
                <div id="dnn_Top_1" class="TNTop1 DNNEmptyPane">
                </div>
                <div id="dnn_Top_2" class="TNTop2 DNNEmptyPane">
                </div>
                <div id="dnn_Top_3" class="TNTop3 DNNEmptyPane">
                </div>
                <div class="clear">
                </div>
                <div id="dnn_Middle_1" class="TNMiddle1 DNNEmptyPane">
                </div>
                <div id="dnn_Middle_2" class="TNMiddle2 DNNEmptyPane">
                </div>
                <div class="clear">
                </div>
                <div id="dnn_Detail_Left" class="TNDetailLeft">
                    <div class="DnnModule DnnModule-TNCategorySolution DnnModule-538"><a name="538"></a>
                        <div id="dnn_ctr538_ContentPane">
                            <!-- Start_Module_538 -->
                            <div id="dnn_ctr538_ModuleContent" class="DNNModuleContent ModTNCategorySolutionC">
                                <div style="clear:both;"></div>
                                <div class="header_danhmuc">
                                    <div class="hearder_tab_home">
                                        <a href="/"></a>
                                    </div>
                                    <div class="header_Shape"></div>
                                    <div class="hearder_tab_level1"><a href="/thong-tin-tre-em">Thông tin trẻ em</a> </div>
                                    <div class="header_warp_right"></div>
                                </div>
                                <div style="clear:both;"></div>
                                <div style="clear:both;"></div>
                                <span id="dnn_ctr538_Main_lblMessage" style="clear:both;"></span>
                            </div><!-- End_Module_538 -->
                        </div>
                        <div style="clear:both;"></div>
                    </div>
                    <div class="DnnModule DnnModule-TNNewsSolution DnnModule-530"><a name="530"></a>
                        <div id="dnn_ctr530_ContentPane">
                            <div id="dnn_ctr530_ModuleContent" class="DNNModuleContent ModTNNewsSolutionC">
                                <input type="hidden" name="dnn$ctr530$Main$UserNewsCategory$_PreviewCategoryId"
                                    id="dnn_ctr530_Main_UserNewsCategory__PreviewCategoryId">
                                <div class="tncndemo_wrap">
                                    <div class="contentx">
                                        <% for(var i = 0; i < data.length; i++){ %>
                                        <% if (i == 0) { %>
                                        <div class="xitem first_item">
                                            <a href="/thong-tin-tre-em/<%= data[i]._id %>" style="display:block;">
                                                <% if (imagesrc[i] !== '') { %>
                                                <img class="ximg" alt="<%= data[i].title %>" src="<%=imagesrc[i]%>">
                                                <% } %>
                                            </a>
                                            <h2>
                                                <a href="/thong-tin-tre-em/<%= data[i]._id %>" class="xlink">
                                                    <%= data[i].name %>
                                                </a>
                                            </h2>
                                            <span class="xsubject" style="word-break: break-word; font-size: 12px;">
                                                <%= data[i].address %></span>
                                            <div style="clear:both;"></div>
                                        </div>
                                        <div class="sep"></div>
                                        <div style="clear:both"></div>
                                        <% } else {%>
                                        <div class="xitem">
                                            <a href="/thong-tin-tre-em/<%= data[i]._id %>" style="display:block;">
                                                <% if (imagesrc[i] !== '') { %>
                                                <img class="ximg" alt="<%= data[i].name %>" src="<%=imagesrc[i]%>">
                                                <% } %>
                                            </a>
                                            <h2>
                                                <a href="/thong-tin-tre-em/<%= data[i]._id %>" class="xlink">
                                                    <%= data[i].name %>
                                                </a>
                                            </h2>
                                            <span class="xsubject" style="word-break: break-word; font-size: 12px;">
                                                <%= data[i].address %></span>
                                            <div style="clear:both;"></div>
                                        </div>
                                        <div class="sep"></div>
                                        <div style="clear:both"></div>
                                        <% } %>
                                        <% } %>
                                        <div style="clear:both"></div>
                                        <nav aria-label="...">
                                            <% if (pages > 0) { %>
                                            <ul class="pagination justify-content-center">
                                                <% if (current == 1) { %>
                                                <li class="page-item disabled"><a class="page-link"
                                                        style="font-size: 10px;">Trang đầu</a></li>
                                                <% } else { %>
                                                <li><a class="page-link" href="<%=link%>/1<%=search%>"
                                                        style="font-size: 10px;">Trang đầu</a></li>
                                                <% } %>
                                                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                                <% if (i !== 1) { %>
                                                <li class="page-item disabled"><a>...</a></li>
                                                <% } %>
                                                <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                                <% if (i == current) { %>
                                                <li class="page-item active"><span class="page-link"
                                                        style="font-size: 10px;">
                                                        <%= i %>
                                                        <span class="sr-only">(current)</span>
                                                        <% } else { %>
                                                <li><a class="page-link" href="<%=link%>/<%= i %><%=search%>"
                                                        style="font-size: 10px;"><%= i %></a>
                                                </li>
                                                <% } %>
                                                <% if (i == Number(current) + 4 && i < pages) { %>
                                                <li class="page-item disabled"><a>...</a></li>
                                                <% } %>
                                                <% } %>
                                                <% if (current == pages) { %>
                                                <li class="page-item disabled"><a class="page-link" style="font-size: 10px;">Trang
                                                        cuối</a></li>
                                                <% } else { %>
                                                <li><a class="page-link" href="<%=link%>/<%= pages %><%=search%>"
                                                        style="font-size: 10px;">Trang cuối</a>
                                                </li>
                                                <% } %>
                                            </ul>
                                            <% } %>
                                        </nav>
                                    </div>
                                    <div style="clear:both;"></div>
                                </div>
                                <div style="clear:both;"></div>
                                <div style="clear:both"></div>
                            </div><!-- End_Module_530 -->
                        </div>
                        <div style="clear:both;"></div>
                    </div>
                </div>
                <%-include("./Detail_Right")%>
                <div class="clear">
                </div>
                <div id="dnn_VideoLeft" class="videoleft DNNEmptyPane">
                </div>
                <div id="dnn_VideoRight" class="videoright DNNEmptyPane">
                </div>
                <div class="clear">
                </div>
                <div id="dnn_VideoHomeTop" class="TNvideotop DNNEmptyPane">
                </div>
                <div class="clear">
                </div>
                <div id="dnn_Video_Left" class="TNVdieoleft DNNEmptyPane">
                </div>
                <div id="dnn_Video_Center" class="TNVideocenter DNNEmptyPane">
                </div>
                <div id="dnn_Video_Right" class="TNVideoright DNNEmptyPane">
                </div>
                <div class="clear">
                </div>
                <div id="dnn_Bottom" class="TNContent DNNEmptyPane">
                </div>
            </div>
            <div class="clear">
            </div>
        </div>
    </div>
    <%-include("./footer")%>
    <div id="scroll-top" style="display: none;">
        <a href="/" tabindex="0">
            <div class="GotoTop">
                Scroll</div>
        </a>
    </div>
    <div id="dnn_Contentpane" class="DNNEmptyPane">
    </div>
    <script type="text/javascript">
        function tninit() {
            jQuery(".smartlistnoheight").each(function () {
                var me = jQuery(this);
                me.customScroll();
            });
            tndate(".dt");
            jQuery(".dt").mask('99/99/9999');
            jQuery('img[src=]').attr('src', 'abc.jpg');
            jQuery('a img.ximg').error(function () {
                jQuery(this).parent().hide();
            });

            jQuery('.imgnews').each(function () {
                var me = jQuery(this);
                me.find('img').error(function () {
                    me.hide();
                });
                var mheight = me.height();
                var mwidth = me.width();
                var mimg = me.find('img:first');
                if (mimg.height() < mheight) {
                    mimg.css("width", "auto");
                    mimg.css("height", mheight);
                }
            });
        }
    </script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            jQuery("#scroll-top").hide();
            jQuery(function () {
                jQuery(window).scroll(function () {
                    if (jQuery(this).scrollTop() > 400) {
                        jQuery('#scroll-top').fadeIn();
                    } else {
                        jQuery('#scroll-top').fadeOut();
                    }
                });
                jQuery('#scroll-top a').click(function () {
                    jQuery('body,html').animate({
                        scrollTop: 0
                    }, 1000);
                    return false;
                });
            });
        });
    </script>
    <script language="javascript" type="text/javascript">

        $(function () {
            $("#amazon_scroller3").amazon_scroller({
                scroller_title_show: 'enable',
                scroller_time_interval: '3000',
                scroller_window_background_color: "none",
                scroller_window_padding: '0',
                scroller_border: "1px solid #c8c8c8",
                scroller_images_width: '140',
                scroller_images_height: '100',
                scroller_title_size: '0',
                scroller_title_color: '#0c6797',
                scroller_show_count: '6',
                directory: 'images'
            });
        });
    </script>

    <input name="ScrollTop" type="hidden" id="ScrollTop" value="106">
    <input name="__dnnVariable" type="hidden" id="__dnnVariable" autocomplete="off"
        value="`{`__scdoff`:`1`,`sf_siteRoot`:`/`,`sf_tabId`:`113`}">
