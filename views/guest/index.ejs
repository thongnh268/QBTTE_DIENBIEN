                                    function loadVideo(seed) {
                                        var scop = jQuery(".mid" + '520');

                                        var x;
                                        if (seed == 1) {
                                            x = scop.find(".xshow:last");
                                            if (x.length > 0 && x.next(".img").length == 0) {
                                                return;
                                            }
                                        } else {
                                            x = scop.find(".xshow:first");
                                            if (x.length > 0 && x.prev(".img").length == 0) {
                                                return;
                                            }
                                        }

                                        scop.find(".xshow").attr("class", "img").hide();

                                        var limit = 4;
                                        if (x.length <= 0) {
                                            x = scop.find(".img:first");
                                            x.addClass("xshow").fadeIn();
                                            limit = limit - 1;
                                        }

                                        for (i = 0; i < limit; i++) {
